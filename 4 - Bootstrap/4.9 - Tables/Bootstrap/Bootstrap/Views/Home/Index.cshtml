@{
    ViewBag.Title = "Home Page";
}

<div class="row">
    <div class="col-md-2 text-center">
        Some tabular data
    </div>
    <div class="col-md-10">
        <table class="table table-condensed table-striped table-hover">
            <caption>Months Of The Year</caption>
            <thead>
                <tr>
                    <th>Month</th>
                    <th>Days</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>January</td>
                    <td>31</td>
                </tr>
                <tr class="active">
                    <td>February</td>
                    <td>28<span class="text-danger">*</span></td>
                </tr>
                <tr class="success">
                    <td>March</td>
                    <td>31</td>
                </tr>
                <tr>
                    <td>April</td>
                    <td>30</td>
                </tr>
                <tr class="danger">
                    <td>May</td>
                    <td>31</td>
                </tr>
                <tr>
                    <td>June</td>
                    <td>30</td>
                </tr>
                <tr>
                    <td>July</td>
                    <td>31</td>
                </tr>
                <tr>
                    <td>August</td>
                    <td>31</td>
                </tr>
                <tr>
                    <td>September</td>
                    <td>30</td>
                </tr>
                <tr>
                    <td>October</td>
                    <td>31</td>
                </tr>
                <tr>
                    <td>November</td>
                    <td>30</td>
                </tr>
                <tr>
                    <td>December</td>
                    <td>31</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>


